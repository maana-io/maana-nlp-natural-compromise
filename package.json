{
  "name": "graphql-boilerplate",
  "scripts": {
    "start": "node -r ./src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "compromise": "^11.12.1",
    "graphql-yoga": "1.16.2",
    "jsonwebtoken": "8.3.0",
    "natural": "^0.6.1",
    "prisma-binding": "2.1.6",
    "uuid4": "^1.1.4"
  },
  "devDependencies": {
    "dotenv": "6.0.0",
    "graphql-cli": "2.16.7",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3",
    "prisma": "1.16.5"
  }
}
